{
  "title": "SSH for Developers",
  "options": {
    "controls": true,
    "progress": true,
    "history": true,
    "center": true,
    "slideNumber": true,
    "touch": true,
    "hideAddressBar": true,
    "mouseWheel": true
  },
  "plugins": {
    "search": true,
    "zoom":   true,
    "menu":   true,
    "notes":  true,
    "highlightjs": true,
    "ga": false
  },
  "serviceWorker": true,
  "slides": [
    [
      {"path": "index.md", "attrs": {"class":"intro"}},
      "01-intro/*.md"
    ],
    ["02-port-forwarding/*md"],
    ["03-ssh-config/*md"],
    "end.html"
  ]
}
